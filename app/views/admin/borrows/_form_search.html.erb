<%= form_for :search, url: search_admin_borrows_path, html: {method: :get} do |f| %>
  <div class="row">
    <div class="col-md-3">
      <div class="form-group">
        <%= f.label t("admin.borrows.email"), class: "control-label mb-10" %>
        <%= f.text_field :email, class: "form-control" %>
      </div>
    </div>
    <div class="col-md-3">
      <div class="form-group">
        <%= f.label t("admin.borrows.book_name"), class: "control-label mb-10" %>
        <%= f.text_field :book_name, class: "form-control" %>
      </div>
    </div>
    <div class="col-md-3 mt-30">
      <div class="form-actions">
        <%= f.submit t("admin.borrows.search"), class: "btn btn-success btn-icon left-icon mr-10 pull-left" %>
        <div class="clearfix"></div>
      </div>
    </div>
  </div>
<% end %>
